{
  "format": "page",
  "id": "article",
  "label": "Article",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "categorie": {
        "type": "Link",
        "config": {
          "label": "Categorie",
          "select": "document",
          "customtypes": ["blog_categorie"]
        }
      },
      "titlearticle": {
        "type": "StructuredText",
        "config": {
          "label": "TitleArticle",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "heading1"
        }
      },
      "descriptionarticle": {
        "type": "StructuredText",
        "config": {
          "label": "DescriptionArticle",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph"
        }
      },
      "date_de_creation": {
        "type": "Date",
        "config": {
          "label": "Date de creation",
          "placeholder": ""
        }
      },
      "picture": {
        "type": "Image",
        "config": {
          "label": "picture",
          "constraint": {},
          "thumbnails": []
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "bruno_text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      },
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      }
    }
  }
}
